{
  "name": "activismebe/database-layering",
  "description": "Laravel Repositories",
  "license": "MIT",
  "homepage": "https://www.activisme.be",
  "keywords": ["laravel", "repository", "repositories", "eloquent", "database"],
  "require": {
    "php": ">=7.0.0",
    "illuminate/support": "^5.4",
    "illuminate/database": "^5.4",
    "illuminate/filesystem": "^5.4",
    "illuminate/console": "^5.4"
  },
  "require-dev": {
    "mockery/mockery": "dev-master@dev",
    "squizlabs/php_codesniffer": "3.*"
  },
  "autoload": {
    "psr-4": {
      "ActivismeBE\\DatabaseLayering\\": "src/ActivismeBE/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ActivismeBE\\DatabaseLayering\\Tests\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.x-dev"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}